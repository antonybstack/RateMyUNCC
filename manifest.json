{
  "manifest_version": 2,
  "name": "RateMyUNCC",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["bg_page.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "script.js"],
      "css": ["styles.css"]
    }
  ]
}
