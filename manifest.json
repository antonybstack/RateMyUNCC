{
  "manifest_version": 2,
  "name": "RateMyUNCC",
  "version": "0.1.3",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["bg_page.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://selfservice.uncc.edu/*"],
      "js": ["jquery.min.js", "script.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": ["*.gif"]
}
